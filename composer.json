{
    "name": "besanek/laravel-firebase-notifications",
    "description": "Thin layer for integrate Firebase Messaging as Laravel Notifications",
    "keywords": ["filesystem", "laravel", "notifications", "firebase", "l5", "l6"],
    "type": "library",
    "require": {
        "illuminate/notifications": "^7.0|^8.0",
        "illuminate/support": "^7.0|^8.0",
        "php": "^7.4",
        "kreait/firebase-php": "~4.18",
        "ext-json": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8|^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.4.2"
    },
    "authors": [
        {
            "name": "Robert Jelen",
            "email": "robert@jelen.dev"
        }
    ],
    "autoload": {
        "psr-4": {
            "Besanek\\LaravelFirebaseNotifications\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Besanek\\LaravelFirebaseNotifications\\FirebaseServiceProvider"
            ]
        }
    }
}
